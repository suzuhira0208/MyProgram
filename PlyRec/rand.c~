#include <stdio.h>
#include <time.h>
#include <stdlib.h>

int main()
{

  double gauss = 0;
  double uniform = 0;
  int j;
  int N = 100;
  

  srand((unsigned int)time(NULL));

  for(i = 0; i < N; i++){
  
    for(j = 0; j < 12; j++){
      uniform = (double)(rand() * 3 / (1.0 + RAND_MAX));
      gauss = gauss + uniform;
    }
    gauss = gauss -6;
    
    printf("%d\n", gauss);
  }


}

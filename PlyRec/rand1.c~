#include <stdio.h>
#include <time.h>
#include <stdlib.h>

int main(){
  double gauss = 0.0;
  double uniform = 0.0;
  int i,j;
  int N = 1000;

  int cnt0 = 0;
  int cnt1 = 0;
  int cnt2 = 0;
  int cnt3 = 0;

  srand((unsigned int)time(NULL));

  for(i = 0; i < N; i++){
   
   
    for(j = 0; j < 12; j++){
      uniform =  (double)rand() /(double) (RAND_MAX);
      
      gauss = gauss + uniform;
    }
    gauss = gauss - 6.0;
  
  
  
    if(gauss < -1.0){
      cnt0 ++;
    }else if(-1.0 <= gauss && gauss < 0.0){
      cnt1 ++;
    }else if(0.0 <= gauss && gauss <= 1.0){
      cnt2 ++;
    }else if(1.0 < gauss){
      cnt3 ++;
    }
  

    
    
    printf("%f\n", gauss);
    gauss = 0.0;
    uniform = 0.0;
  }
  printf("%d\n%d\n%d\n%d\n",cnt0, cnt1, cnt2, cnt3);


}
